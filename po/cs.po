msgstr "_Systémové nastavení"

#: ../data/geany.glade.h:110
msgid "Images _and text"
msgstr "Ikony _a text"

#: ../data/geany.glade.h:111
msgid "_Images only"
msgstr "Pouze _ikony"

#: ../data/geany.glade.h:112
msgid "_Text only"
msgstr "Pouze _text"

