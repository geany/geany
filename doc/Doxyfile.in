MULTILINE_CPP_IS_BRIEF = NO
INHERIT_DOCS           = YES
SEPARATE_MEMBER_PAGES  = NO
TAB_SIZE               = 4
# ALIASES taken from pidgin
ALIASES                = "signal=- @ref  " \
                         "signaldef=@subsection  " \
                         "endsignaldef=  " \
                         "signalproto=@code  " \
                         "endsignalproto=@endcode  " \
                         "signaldesc=" \
                         "signals=@b Signals:  " \
                         "endsignals=  "
